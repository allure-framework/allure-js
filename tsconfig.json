  "compilerOptions": {
    "target": "es6",
    "lib": ["es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "declaration": true,
    "strict": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "removeComments": true
  },
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": false
}
