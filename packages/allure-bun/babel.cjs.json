{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "rewriteImportExtensions": true
      }
    ],
    [
      "@babel/preset-env",
      {
        "modules": "commonjs"
      }
    ]
  ],
  "plugins": ["babel-plugin-add-module-exports"],
  "targets": {
    "esmodules": false,
    "node": 18
  }
}