{
  "presets": [
    [
      "@babel/preset-typescript",
      {
        "rewriteImportExtensions": true
      }
    ],
    ["@babel/preset-env", { "modules": "commonjs" }]
  ],
  "plugins": ["babel-plugin-add-module-exports", "babel-plugin-transform-import-meta"],
  "targets": {
    "esmodules": false,
    "node": 18
  }
}
