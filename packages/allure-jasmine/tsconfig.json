{
  "extends": "../../tsconfig.json",
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./test/**/*"
  ],
  "compilerOptions": {
    "skipLibCheck": true,
    "declaration": true,
    "emitDeclarationOnly": true,
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "lib": [
      "dom",
      "esnext"
    ],
    "types": [
      "node",
      "jasmine"
    ],
    "outDir": "./dist"
  }
}
