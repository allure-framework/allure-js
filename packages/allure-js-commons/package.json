{
  "name": "allure-js-commons",
  "version": "2.0.0-beta.11",
  "description": "Allure JS Commons",
  "license": "Apache-2.0",
  "author": "Ilya Korobitsyn <mail@korobochka.org>",
  "repository": "https://github.com/allure-framework/allure-js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "eslint ./src ./index.ts --ext .ts",
    "lint:fix": "eslint ./src ./index.ts --ext .ts --fix",
    "compile": "tsc",
    "build": "npm run lint && npm run compile",
    "prepublishOnly": "npm run clean && npm run build",
    "generate-report": "allure generate ./out/allure-results -o ./out/allure-report --clean",
    "runTest": "jasmine ./dist/test/*.js",
    "runTestDebug": "node $NODE_DEBUG_OPTION ./node_modules/.bin/jasmine ./dist/test/*.js",
    "test": "rimraf ./out ; npm run runTest"
  },
  "devDependencies": {
    "@types/jasmine": "^3.8.2",
    "@types/mkdirp": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "jasmine": "^3.8.0"
  },
  "dependencies": {
    "mkdirp": "^1.0.4",
    "properties": "^1.2.1",
    "uuid": "^8.3.0"
  }
}
